{
  "name": "@ltipton/react-gherkin-editor",
  "version": "0.0.1",
  "description": "A Gherkin language editor for React.",
  "homepage": "https://github.com/ltipton/react-gherkin-editor",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ltipton/react-gherkin-editor"
  },
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "author": "SmartBear Software (Modified by Lance Tipton)",
  "license": "MIT",
  "files": [
    "build"
  ],
  "scripts": {
    "start": "webpack serve --client-log-level silent",
    "pretty": "prettier --write .",
    "test": "jest",
    "lint": "concurrently yarn:eslint yarn:type-check",
    "eslint": "eslint --ext js,jsx,ts,tsx .",
    "type-check": "tsc --noEmit",
    "build": "concurrently yarn:build:types yarn:build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir build --extensions \".js,.jsx,.ts,.tsx\"",
    "prepack": "rm -rf build && yarn build",
    "postpack": "rm -rf build",
    "push": "git push --follow-tags",
    "postpublish": "echo The new package has been released! Run \\\"yarn push\\\" to push the release commit and tag."
  },
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.13",
    "@babel/eslint-parser": "^7.12.13",
    "@babel/eslint-plugin": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.12.13",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/react": "^17.0.1",
    "@typescript-eslint/parser": "^4.15.0",
    "babel-loader": "^8.2.2",
    "buffer": "^6.0.3",
    "concurrently": "^5.3.0",
    "eslint": "^7.19.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.22.0",
    "html-webpack-plugin": "^5.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "process": "^0.11.10",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "react-select-event": "^5.1.0",
    "stream-browserify": "^3.0.0",
    "typescript": "^4.1.4",
    "webpack": "^5.21.2",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@cucumber/gherkin": "^17.0.1",
    "ace-builds": "^1.4.12",
    "react-ace": "^9.3.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0 <18.0.0",
    "react-dom": ">=16.8.0 <18.0.0"
  }
}
